<div class="container">
<%= simple_form_for @cocktail do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Create your cocktail", class: "btn btn-primary" %>
<%= link_to "Cancel and get back", cocktails_path, class: "btn btn-primary"  %>
<% end %>
</div>
